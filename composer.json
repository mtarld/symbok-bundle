{
  "name": "mtarld/symbok-bundle",
  "type": "symfony-bundle",
  "description": "Symbok Annotation Bundle",
  "keywords": [
    "symfony",
    "bundle",
    "symfony-bundle",
    "annotation",
    "lombok",
    "annotation-processing",
    "doctrine"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mathias Arlaud",
      "email": "mathias.arlaud@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "nikic/php-parser": "4.*",
    "phpdocumentor/reflection-docblock": "^4.3",
    "doctrine/orm": "~2.7"
  },
  "autoload": {
    "psr-4": {
      "Mtarld\\SymbokBundle\\": ""
    }
  },
  "require-dev": {
    "symfony/framework-bundle": "^4.3",
    "phpunit/phpunit": "~7.5",
    "symfony/yaml": "<4.4",
    "friendsofphp/php-cs-fixer": "~2.14"
  },
  "conflict": {
    "symfony/framework-bundle": "<3.0||>=4.4"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit",
    "clean-code": "./vendor/bin/php-cs-fixer fix ."
  }
}
