{
  "name": "mtarld/symbok-bundle",
  "type": "symfony-bundle",
  "description": "Symbok Annotation Bundle",
  "keywords": [
    "symfony",
    "bundle",
    "symfony-bundle",
    "annotation",
    "lombok",
    "annotation-processing",
    "doctrine"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mathias Arlaud",
      "email": "mathias.arlaud@gmail.com"
    }
  ],
  "require": {
    "php": "~7.2.5|^7.3|^7.4",
    "nikic/php-parser": "^4.2",
    "phpdocumentor/reflection-docblock": "^4.3|^5.0",
    "doctrine/annotations": "^1.8",
    "doctrine/collections": "^1.5",
    "doctrine/dbal": "^2.9",
    "doctrine/orm": "^2.7",
    "symfony/inflector": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Mtarld\\SymbokBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mtarld\\SymbokBundle\\Tests\\": "tests/",
      "App\\": "tests/Fixtures/App/src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4",
    "symfony/framework-bundle": "^4.4|^5.0",
    "symfony/yaml": "^4.4|^5.0",
    "friendsofphp/php-cs-fixer": "^2.15",
    "phpmd/phpmd": "^2.8",
    "symfony/monolog-bundle": "^3.5",
    "mikey179/vfsstream": "~1.6.8",
    "rskuipers/php-assumptions": "^0.8.0",
    "sebastian/phpcpd": "^4.1|^5.0",
    "vimeo/psalm": "^3.10",
    "psalm/plugin-phpunit": "^0.9.2",
    "symfony/phpunit-bridge": "^5.0",
    "doctrine/doctrine-bundle": "^2.0"
  },
  "conflict": {
    "symfony/framework-bundle": "<4.0|>=6.0"
  }
}
